<div class="container p-4">
    <div class="row">
         <div class="col-md-4 mx-auto">
             {{#each links}}
             <div class="card">
               <div class="card-body">
                  <form action="/videos/add/url/{{url}}" method="POST" enctype="multipart/form-data">
                     <div class="form-group">
                     </div>
                     <div class="form-group"> 
                      <input type="text" class="form-control" name="name" placeholder="Nombres del Autor Video" autofocus>
                         </div>
                      <div class="form-group">
                          <input type="text" name="lastname" class="form-control" placeholder="Apellidos">
                      </div>
                      <div class="form-group">
                          <input type="email" name="email" placeholder="Email" class="form-control">
                     </div>
                      <div class="form-group">
                          <textarea name="message" rows="2" class="form-control" placeholder="Mensaje"></textarea>
                      </div>
                       <div class="form-group">
                           Video
                           <input type="file" id="file" name="originalvideo" >
                      </div>
                      <div class="form-group">
                          <button class="btn btn-success btn-block">
                              SAVE
                          </button>
                      </div>
                  </form>
                 </div> 
             </div>
              {{/each}}
         </div>
    </div>
</div>